import java.util.Scanner;
class one
{
	String name=new String();
	int id;
	int sem;
	
	public static void main(String[] a)
	{
		one s1=new one();
		one s2=new one();
		one s3=new one();


		s1.id=1;
		s2.id=2;
		s1.sem=3;
		s2.sem=4;
		s1.name="kalpesh";
		s2.name="kapxx";
		System.out.println("first sudent id is:"+ s1.id);
		System.out.println("first sudent sem is:"+ s1.sem);
		System.out.println("first sudent name is:"+ s1.name);
		System.out.println("");


		System.out.println("first sudent id is:"+ s2.id);
		System.out.println("first sudent sem is:"+ s1.sem);
		System.out.println("first sudent name is:"+ s1.name);
		System.out.println("sucess");

	}

}

