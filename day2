import java.util.Scanner;
class sum
{
	
	public static void main(String[] a)
	{

	int num1=10 ;
	int num2=20 ;
	

		if(num1<num2)
		{
			System.out.println("num2 is max");
		}
		else if(num1>num2)
		{
			System.out.println("num1 is max");
		}

	}

}
