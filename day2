class Student
{
	int id;
	int sem;
	Student()
	{
		id=3;
		sem=4;
	}
	Student(int i,int s)
	{
		id=i;0 .0
		sem=s;
	}

	public static void main(String[] a)
	{


	Student s1=new Student(1,3);
	System.out.println("student id is"+s1.id);


	}
}
