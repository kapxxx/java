class outer1
{
	
	public static void main(String[] a)
	{

	outer:
		for(int i=0;i<10;i++)
		{
			for(int j=0;j<10;j++)
			{
				if(j==1)
				
					break ;
				System.out.println("j value is:"+j);
				
			}
		}

	}
}
